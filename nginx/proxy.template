events {
    worker_connections 1024;
}

http {
    server {
        listen 80;
        server_name ${API_SERVER_NAME};
        location / {
            proxy_pass ${GATEWAY_URL};
        }
    }

    server {
        listen 80;
        server_name ${IMAGEPROXY_SERVER_NAME};
        location / {
            proxy_pass ${IMAGEPROXY_URL};
        }
    }
}
